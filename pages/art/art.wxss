.weui-grid-zdy {
position:relative;
float:left;
padding:5px 5px;
width:50%;
box-sizing:border-box;
}
.weui-panel-zdy{
  background-color:#fff;
  position:relative;
  overflow:hidden;
}
.weui-grid_zdy {
  position:relative;
  float:left;
  padding:40px 10px;
  width:49%;
  box-sizing:border-box;
  border-radius:8px;
  margin-left:.5%;
  margin-right:.5%;
  margin-bottom:6px;
  background-size:100% 100%;
}
.weui-search-bar__form_zdy {
  position:relative;
  -webkit-box-flex:1;
  -webkit-flex:auto;
  flex:auto;
  border-radius:5px;
  background:#fff;
}
.weui-search-bar_zdy {
  position:relative;
  padding:8px 10px;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  box-sizing:border-box;
}
.art-claszz-font{
  font-weight:bold;
  color:#ffffff;
}
